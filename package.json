{
  "name": "@scandinavia/nestjs-libs",
  "version": "1.1.5",
  "description": "Useful libraries for scandinaviatech",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "/lib"
  ],
  "author": "Rami Kass Youssef",
  "license": "MIT",
  "private": false,
  "scripts": {
    "pre-publish": "yarn format && yarn build",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc && tscpaths -p tsconfig.json -s ./src -o ./lib/esm",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs && tscpaths -p tsconfig.json -s ./src -o ./lib/cjs",
    "format": "prettier --write \"src/**/*.ts\"",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "cls-hooked": "^4.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tscpaths": "0.0.9"
  },
  "peerDependencies": {
    "@casl/ability": "^5.4.3",
    "@casl/mongoose": "^6.0.0",
    "@nestjs/common": "^8.2.4",
    "@nestjs/swagger": "^5.1.5",
    "mongodb": "^4.3.0",
    "mongoose": "^6.1.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "jsonwebtoken": "^8.5.1",
    "rxjs": "^7.5.1"
  },
  "devDependencies": {
    "@casl/ability": "^5.4.3",
    "@casl/mongoose": "^6.0.0",
    "@nestjs/common": "^8.2.4",
    "@nestjs/swagger": "^5.1.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.3.0",
    "mongoose": "^6.1.4",
    "prettier": "^2.3.2",
    "rxjs": "^7.5.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
